{
  "name": "mosaic",
  "version": "1.0.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "dependencies": {
    "case": "^1.6.3",
    "electron": "^25.5.0",
    "js-common": "https://github.com/davidzeng0/js-common.git#dist",
    "mongodb": "^5.7.0",
    "protobuf-ts": "https://github.com/davidzeng0/protobuf-ts.git#dist",
    "protobufjs": "^7.2.2",
    "yargs": "^17.7.1"
  },
  "devDependencies": {
    "@types/node": "^18.14.0",
    "@types/node-fetch": "^2.6.4",
    "@types/yargs": "^17.0.24",
    "git-publish": "^2.0.0",
    "tsup": "^7.2.0",
    "typescript": "^5.1.6"
  },
  "files": [
    "dist"
  ],
  "bin": {
    "mosaic_login": "./login",
    "mosaic_export_credentials": "./export_credentials",
    "mosaic_import_credentials": "./import_credentials"
  },
  "scripts": {
    "build": "tsup-node --entry.index src/index.ts --entry.export_credentials src/tools/export_credentials.process.ts --entry.import_credentials src/tools/import_credentials.process.ts --minify --keep-names --target esnext --dts --treeshake",
    "clean": "rm -rf dist",
    "cleanall": "yarn clean && rm -rf node_modules yarn.lock package-lock.json",
    "git-publish": "git-publish -b dist"
  }
}